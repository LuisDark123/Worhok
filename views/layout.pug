        ul  
          li
            a(href="./paginas-web.html")
              div
                i(class="icon-layout")
                small Paginas Web
              i(class="icon-arrow-right icon-nav-alternate")
            a(href="./sitios-web.html" style="display: none;")
              div
                i(class="icon-globe")
                small Sitios Web
              i(class="icon-arrow-right icon-nav-alternate")
            a(href="./paginas-web.html" style="display: none;")
              div
                i(class="icon-box")
                small Proyectos
              i(class="icon-arrow-right icon-nav-alternate")
        
        div(class="nav-alternate-social")
            a(href="https://www.facebook.com/Worhok/" target="_blank")  
              i(class="icon-facebook")
            a(href="https://www.instagram.com/worhok/" target="_blank")
