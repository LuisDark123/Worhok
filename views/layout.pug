      div(class="nav-social")
        a(href="https://www.facebook.com/Worhok/" target="_blank")
          i(class="icon-facebook")
        a(href="https://www.instagram.com/worhok/" target="_blank")
          i(class="icon-instagram")
        a(href="https://www.youtube.com/channel/UC5MDMq0xXst-sY0heK0pDjA" target="_blank")
          i(class="icon-youtube")

      i(class="icon-menu nav-button" id="nav-button")

